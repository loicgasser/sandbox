{
  "type": "single",
  "geomType": "point",
  "rotation": "station_symbol",
  "vectorOptions": {
    "type": "icon",
    "src": "https://raw.githubusercontent.com/loicgasser/sandbox/master/arrow.png",
    "label": {
          "template": "${station_name}\n ${value} mm",
          "text": {
            "textAlign": "center",
            "textBaseline": "middle",
            "font": "bold 11px FrutigerNeueW02-Regular, Times, sans-serif",
            "scale": 1.25,
            "offsetY": -28,
            "stroke": {
              "color": "#7B9FB2",
              "width": 3
            },
            "fill": {
              "color": "rgb(254,254,254)"
            }
          }
  }
}
